<h1>Find a Flickr Feed</h1>
<h4>(Examples: "198780121@N08" or "polybazze" )</h4>
<%= render "form" %>
<%= 'No user was found with that ID.' unless @user_nsid %>

<% if @feed_urls %>

	<ul class="container">

  <% @feed_urls.each do |url| %>
		<li class="img-wrapper">
			<span class="img"><%= image_tag(url) %></span>
		</li>
  <% end %>

	</ul>
<% end %>

